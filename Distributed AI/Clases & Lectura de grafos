ef add_node(self, node_name):\n","    self.nodes[node_name] = len(self.nodes)\n","\n","  def add_edge(self, node_a, node_b, dist, is_directed=False):\n","    self.distance_matrix[self.nodes[node_a]][self.nodes[node_b]] = dist\n","    if not is_directed: self.distance_matrix[self.nodes[node_b]][self.nodes[node_a]] = dist\n","\n","  @staticmethod\n","  def from_txt(path, is_directed=False):\n","    g = Graph()\n","    raw_edges = []\n","    with open(path, \"r\") as file:\n","      for line in file.readlines():\n","        node_a, node_b, dist = line.split()\n","        if node_a not in g.nodes: g.add_node(node_a)\n","        if node_b not in g.nodes: g.add_node(node_b)\n","        raw_edges.append((node_a, node_b, dist))\n","    g.__init_distance_matrix(len(g.nodes))\n","    for a, b, d in raw_edges:\n","      g.add_edge(a, b, d, is_directed=is_directed)\n","    return g"]},{"cell_type":"code","source":["g1 = Graph.from_txt(\"Prueba.txt\", is_directed=True)"],"metadata":{"id":"Fk_nhLWdsEf1"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["g1.distance_matrix"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"n3L154p4uOb3","executionInfo":{"status":"ok","timestamp":1697849715127,"user_tz":240,"elapsed":2,"user":{"displayName":"Carlos Bienvenido Ogando Mont치s (Carlitos5336)","userId":"10505299496410393010"}},"outputId":"300acf9c-5787-4d39-a46e-6fcf4b66703a"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([[inf,  3.,  2.,  4.],\n","       [inf, inf, inf, inf],\n","       [inf, inf, inf, inf],\n","       [inf, inf, inf, inf]])"]},"metadata":{},"execution_count":112}]}]}